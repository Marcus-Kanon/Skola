@page "/search"
@using Test_matsidaBlazor.Data

<Section_BodyHeader Header="Sök!" />

<section class="bg-light">
    <div class="container">
        <SearchInput SearchAction="name" Search="@test" />
    </div>
</section>

@code {

    [Parameter]
    public string GetSearch { get; set; } = "";

    public string test { get; set; } = "";

    protected override void OnParametersSet()
    {
        test = GetSearch;

        base.OnParametersSet();
    }
}
